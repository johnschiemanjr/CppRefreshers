#ifndef TILE_H_
#define TILE_H_

#include <string>

using namespace std;

class Tile
{
public:
	Tile();
	virtual ~Tile();
private:
	char status;
};

char Tile::get_status() const
{
	return status;
}

void Tile::set_status(char status)
{
	this->status = status;
}


#endif /* TILE_H_ */
