#include "Contact.h"
#include <string>

using namespace std;

Contact::Contact(string first, string last, string number)
{
	this->first_name = first;
	this->last_name = last;
	this->phone_number = number;
}

Contact::~Contact()
{
}

void get_entry()
{
	cout << this->first_name << " " << last
}

