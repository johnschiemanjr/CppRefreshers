#include "Tile.h"
#include <string>
#include <iostream>

using namespace std;

Tile::Tile(char status)
{
	this->status = status;
}

Tile::~Tile()
{
}

char Tile::get_status() const
{
	return status;
}

void Tile::set_status(char status)
{
	this->status = status;
}


