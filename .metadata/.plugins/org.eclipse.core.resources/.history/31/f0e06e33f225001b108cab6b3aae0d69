#ifndef CONTACT_H_
#define CONTACT_H_

#include <string>

using namespace std;

class Contact
{
public:
	string first_name;
	string last_name;
	string phone_number;
	Contact(string first, string last, string number);
	virtual ~Contact();
};

#endif /* CONTACT_H_ */
